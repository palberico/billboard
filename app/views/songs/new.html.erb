<h1>New Song Form</h1>

<%= form_for @songs do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :genre %>
    <%= f.text_field :genre %>

    <%= f.label :duration %>
    <%= f.text_field :duration %>

    <%= f.label :album %>
    <%= f.text_field :album %>

    <%= f.submit%>
<% end %>